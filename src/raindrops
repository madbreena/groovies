class Raindrops {

    def convert(num) {

        def sound = ''
        if (num % 3 == 0) {
            sound = sound + 'Pling'
        }
        if (num % 5 == 0 ) {
            sound =sound + 'Plang'
        }
        if (num % 7 == 0 ) {
            sound =sound + 'Plong'
        }
        if ((num % 3 !=0) && (num % 5 !=0) && (num % 7 !=0)){
            sound =num.toString()
        }
        sound
    }

}